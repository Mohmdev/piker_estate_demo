{
  "name": "webora-estates",
  "version": "1.0.0-beta.4",
  "private": true,
  "license": "MIT",
  "type": "module",
  "author": "Mohmdev",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mohmdev/webora-estates"
  },
  "scripts": {
    "ci": "pnpm i --frozen-lockfile",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace i",
    "update-deps": "rm node_modules pnpm-lock.yaml -rf && pnpm i",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace i",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "start": "next start -p 3000",
    "start:private": "cross-env next start --hostname :: -p 3000",
    "dev": "next dev --turbopack -p 3000",
    "dev:inspect": "NODE_OPTIONS='--inspect' next dev --turbopack -p 3000",
    "devsafe": "rm -rf .next && cross-env next dev --turbopack -p 3000",
    "lint": "biome lint ./src",
    "lint:next": "next lint",
    "lint:fix": "biome lint --write ./src",
    "format:src": "biome format --write ./src",
    "fix:ci": "biome ci --formatter-enabled=true --linter-enabled=true --organize-imports-enabled=true --assists-enabled=true",
    "generate:importmap": "payload generate:importmap",
    "generate:schema": "payload generate:db-schema",
    "generate:graphql": "payload-graphql generate:schema",
    "generate:types": "payload generate:types",
    "migrate": "payload migrate",
    "migrate:status": "payload migrate:status",
    "migrate:fresh": "payload migrate:fresh",
    "migrate:create": "payload migrate:create",
    "db:reset": "tsx src/services/database/db-reset.ts"
  },
  "dependencies": {
    "@payloadcms/db-vercel-postgres": "latest",
    "@payloadcms/email-resend": "latest",
    "@payloadcms/live-preview-react": "latest",
    "@payloadcms/next": "latest",
    "@payloadcms/plugin-form-builder": "latest",
    "@payloadcms/plugin-nested-docs": "latest",
    "@payloadcms/plugin-redirects": "latest",
    "@payloadcms/plugin-search": "latest",
    "@payloadcms/plugin-seo": "latest",
    "@payloadcms/richtext-lexical": "latest",
    "@payloadcms/storage-vercel-blob": "latest",
    "@payloadcms/ui": "latest",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/postcss": "^4.0.5",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.66.0",
    "@vercel/toolbar": "^0.1.30",
    "babel-plugin-react-compiler": "19.0.0-beta-30d8a17-20250209",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.10",
    "geist": "^1.3.1",
    "graphql": "^16.8.2",
    "juice": "^11.0.0",
    "lucide-react": "^0.475.0",
    "mapbox-gl": "^3.10.0",
    "motion": "^12.4.7",
    "next": "^15.1.6",
    "next-sitemap": "^4.2.3",
    "payload": "latest",
    "payload-admin-bar": "^1.0.6",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "7.54.2",
    "react-icons": "^5.4.0",
    "react-use-measure": "^2.1.7",
    "sharp": "0.32.6",
    "swiper": "^11.2.2",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/ejs": "^3.1.5",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.4.7",
    "globals": "^15.14.0",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=18.20.2 <23.0.0",
    "pnpm": ">=9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
